from typing import Any, Callable, Union

from eventlet.support import greenlets as greenlet

class GreenThread(greenlet.greenlet):
    def __init__(self, parent: greenlet.greenlet) -> None: ...


def spawn(func: Callable, *args: Any, **kwargs: Any) -> GreenThread: ...
def spawn_n(func: Callable, *args: Any, **kwargs: Any) -> greenlet.greenlet: ...

def sleep(seconds: Union[float, int] = ...) -> None: ...
